<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhd.mapper.ConsumptionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhd.pojo.Consumption">
        <id column="id" property="id" />
        <result column="o_id" property="oId" />
        <result column="u_id" property="uId" />
        <result column="con_amount" property="conAmount" />
        <result column="pay_amount" property="payAmount" />
        <result column="c_id" property="cId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, o_id AS oId, u_id AS uId, con_amount AS conAmount, pay_amount AS payAmount, c_id AS cId
    </sql>

</mapper>
