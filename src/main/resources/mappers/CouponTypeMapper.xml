<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhd.mapper.CouponTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhd.pojo.CouponType">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="par_value" property="parValue" />
        <result column="condition_amount" property="conditionAmount" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, par_value AS parValue, condition_amount AS conditionAmount, start_time AS startTime, end_time AS endTime
    </sql>

</mapper>
