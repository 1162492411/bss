<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhd.mapper.VipMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhd.pojo.Vip">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="vip_time" property="vipTime" />
        <result column="operate_time" property="operateTime" />
        <result column="amount" property="amount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id AS userId, vip_time as vipTime, operate_time AS operateTime, amount
    </sql>

</mapper>